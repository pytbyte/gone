.notify-icon span{
  display: inline-block;
  border: 1px solid orange;
  border-radius: 50%;
  height: 24px; 
  width: 24px;
  animation: pulse .6s 4 forwards;
  transform-origin: center center;
  background-image: "static/images/default/default.jpg";
}
@keyframes pulse{
  0%{
    transform: scale(5.5);
    opacity: .2;
  }
  50%{
    opacity: .8;
  }
  100%{
    transform: scale(.1);
    opacity: 1;
  }
}
