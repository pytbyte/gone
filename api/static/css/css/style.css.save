#toolbar {
  background: rgba(255, 255, 255, 1);
  opacity: .6;
  width: 200px;
  height: 600px;
  height: 100vh;
  position: absolute;
  left: -200px;
  z-index: 1;
  transition: .5s left;
  padding: 20px;
  box-sizing: border-box;
}

#toolbar.open {
  left: 0;
  opacity: .95;
}

#toolbar .hamburger {
  height: 100px;
  width: 70px;
  background: #000;
  box-shadow: 1px 0 2px rgba(0, 0, 0, .3);
  position: absolute;
  right: -20px;
  top: 40%;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
  border: 1px solid #333;
  cursor: pointer;
}

.hamburger span {
  color: #fff;
  display: inline-block;
  position: relative;
  transform: rotate(90deg);
  top: 39px;
  left: -21px;
}

